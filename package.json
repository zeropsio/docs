{
  "name": "docs-root",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build --env-mode=loose",
    "build:docs": "turbo run build --filter=docs",
    "start": "turbo run start:monorepo",
    "dev": "turbo run dev:monorepo",
    "lint": "turbo run lint",
    "lint:content": "turbo run lint:content"
  },
  "dependencies": {
    "@babel/core": "7.25.2",
    "autoprefixer": "10.4.20",
    "eslint": "9.9.1",
    "postcss": "8.4.44",
    "prettier": "3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.10",
    "tsconfig": "1.0.0",
    "turbo": "2.1.1",
    "webpack": "5.94.0"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "packageManager": "yarn@1.22.22"
}
